{
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "requireConfigFile": false
    },
    "overrides": [{
        "files": [".*.js", "test/**"],
        "rules": {
            "node/no-unsupported-features/es-syntax": "off",
            "node/no-unsupported-features/node-builtins": "off"
        }
    }],
    "extends": [
        "plugin:node/recommended",
        "plugin:putout/recommended"
    ],
    "plugins": [
        "putout",
        "node"
    ]
}
